<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.fh.dao.SysUserDao">
	<select id="queryObject" resultType="com.fh.entity.SysUser">
		select id,user_name from jdbc_user where user_name = #{userName}
	</select>
	

</mapper>